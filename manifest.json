{
	"name": "Scholzly",
	"version": "1",
	"description": "So so so...",
	"background": {
		"page": "background.html"
	},
	"manifest_version": 2,
	"browser_action": {
		"default_icon": "img/So_32x32.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"19": "img/So_19x19.png",
		"32": "img/So_32x32.png",
		"48": "img/So_48x48.png",
		"128": "img/So_128x128.png"
	},
	"content_scripts": [{
		"js": ["jquery.min.js", "main.js"],
		"css": ["background.css"],
		"matches": ["http://*/*", "https://*/*", "file:///*"]
	}],
	"permissions": ["storage"]
	// "popup": ""
}